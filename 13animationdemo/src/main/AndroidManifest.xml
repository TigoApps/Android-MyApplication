<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xcode126.animationdemo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".TweenActivity" />
        <activity android:name=".PropertyActivity" />
        <activity android:name=".FrameActivity" />
        <activity android:name=".tween.SingleCodeActivity" />
        <activity android:name=".tween.ComplexCodeActivity" />
        <activity android:name=".other.ViewAnimatorActivity" />
        <activity android:name=".tween.SingleXmlActivity" />
        <activity android:name=".tween.ComplexXmlActivity" />
        <activity android:name=".tween.AnimationListenerActivity" />
        <activity android:name=".tween.LayoutAnimationsControllerActivity" />
        <activity android:name=".frame.ChangePostionFrameActivity" />
        <activity android:name=".property.ObjectAnimatorActivity" />
        <activity android:name=".property.ValueAnimatorActivity" />
        <activity android:name=".property.AnimatorSetActivity" />
        <activity android:name=".ScanActivity"></activity>
    </application>

</manifest>