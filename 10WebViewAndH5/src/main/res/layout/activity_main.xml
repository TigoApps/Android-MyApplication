<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.xcode126.webviewandh5.MainActivity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:layout_gravity="center_horizontal"
        android:background="@color/colorPrimary"
        android:gravity="center"
        android:text="WebView与JS或者H5互调"
        android:textColor="@color/colorAccent"
        android:textSize="22sp" />

    <Button
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="200dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="jsCallAndroidClick"
        android:text="JS调取Android原生"
        android:textColor="@color/colorAccent"
        android:textSize="20sp" />

    <Button
        android:layout_width="300dp"
        android:layout_height="50dp"
        android:layout_gravity="center_horizontal"
        android:layout_marginTop="50dp"
        android:background="@color/colorPrimaryDark"
        android:onClick="AndroidCallJsClick"
        android:text="Android原生调取JS"
        android:textColor="@color/colorAccent"
        android:textSize="20sp" />

</LinearLayout>
